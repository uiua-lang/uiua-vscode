{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "Uiua",
	"firstLineMatch": "^#!/.*\\buiua\\b",
	"fileTypes": [
		"ua"
	],
	"patterns": [
		{
			"include": "#comments"
		},
		{
			"include": "#strings-multiline-format"
		},
		{
			"include": "#strings-multiline"
		},
		{
			"include": "#strings-format"
		},
		{
			"include": "#strings-normal"
		},
		{
			"include": "#characters"
		},
		{
			"include": "#labels"
		},
		{
			"include": "#module_delim"
		},
		{
			"include": "#strand"
		},
		{
			"include": "#stack"
		},
		{
			"include": "#noadic"
		},
		{
			"include": "#monadic"
		},
		{
			"include": "#dyadic"
		},
		{
			"include": "#mod1"
		},
		{
			"include": "#mod2"
		},
		{
			"include": "#idents"
		},
		{
			"include": "#numbers"
		}
	],
	"repository": {
		"idents": {
			"name": "variable.parameter.uiua",
			"match": "\\b[a-zA-Z]+['′″‴]*(₋?[₀₁₂₃₄₅₆₇₈₉⌞⌟]|,`?\\d+)*[!‼]*\\b"
		},
		"comments": {
			"name": "comment.line.uiua",
			"match": "(#.*$|$[a-zA-Z]*)"
		},
		"strings-normal": {
			"name": "constant.character.escape",
			"begin": "\"",
			"end": "\"",
			"patterns": [
				{
					"name": "string.quoted",
					"match": "\\\\[\\\\\"0nrtW]"
				}
			]
		},
		"strings-format": {
			"name": "constant.character.escape",
			"begin": "\\$\"",
			"end": "\"",
			"patterns": [
				{
					"name": "string.quoted",
					"match": "\\\\[\\\\\"0nrtW_]"
				},
				{
					"name": "constant.numeric",
					"match": "(?<!\\\\)_"
				}
			]
		},
		"strings-multiline": {
			"name": "constant.character.escape",
			"begin": "\\$ ",
			"end": "$"
		},
		"strings-multiline-format": {
			"name": "constant.character.escape",
			"begin": "\\$\\$ ",
			"end": "$",
			"patterns": [
				{
					"name": "constant.numeric",
					"match": "(?<!\\\\)_"
				}
			]
		},
		"characters": {
			"name": "constant.character.escape",
			"match": "@(\\\\(x[0-9A-Fa-f]{2}|u[0-9A-Fa-f]{4}|.)|.)"
		},
		"labels": {
			"name": "label.uiua",
			"match": "\\$[a-zA-Z]*"
		},
		"numbers": {
			"name": "constant.numeric.uiua",
			"match": "[`¯]?(\\d+|η|π|τ|∞|eta|pi|tau|inf(i(n(i(t(y)?)?)?)?)?)([./]\\d+|e[+-]?\\d+)?"
		},
		"strand": {
			"name": "comment.line",
			"match": "(_|‿)"
		},
		"module_delim": {
			"match": "---"
		},
		"stack": {
			"match": "[∘◌?∘]|(?<![a-zA-Z$])(id(e(n(t(i(t(y)?)?)?)?)?)?|po(p)?|arg(s)?|id(e(n(t(i(t(y)?)?)?)?)?)?)(?![a-zA-Z])"
		},
		"noadic": {
			"name": "entity.name.tag.uiua",
			"match": "[⚂]|(?<![a-zA-Z$])(rand(o(m)?)?|now|timezone|os|osfamily|arch|dllext|exeext|pathsep|numprocs|&b|&sc|&ts|&args|&clip|&asr|&camlist|&camlist|numprocs|osfamily|timezone|pathsep|exeext|dllext|&clip|&args|&asr|arch|&ts|&sc|now|&b|os)(?![a-zA-Z])"
		},
		"monadic": {
			"name": "string.quoted",
			"match": "[¬±¯`⌵⨪√ₑ∿⌊⌈⁅⧻△⇡⊢⊣⇌♭¤⋯⍉⍆⍏⍖⊚◴⊛⧆□⋕]|(?<![a-zA-Z$])(not|sig(n)?|neg(a(t(e)?)?)?|abs(o(l(u(t(e( (v(a(l(u(e)?)?)?)?)?)?)?)?)?)?)?|rec(i(p(r(o(c(a(l)?)?)?)?)?)?)?|sqr(t)?|exp(o(n(e(n(t(i(a(l)?)?)?)?)?)?)?)?|sin(e)?|flo(o(r)?)?|cei(l(i(n(g)?)?)?)?|rou(n(d)?)?|len(g(t(h)?)?)?|sha(p(e)?)?|ran(g(e)?)?|fir(s(t)?)?|las(t)?|rev(e(r(s(e)?)?)?)?|des(h(a(p(e)?)?)?)?|fix|bit(s)?|tra(n(s(p(o(s(e)?)?)?)?)?)?|sor(t)?|ris(e)?|fal(l)?|whe(r(e)?)?|ded(u(p(l(i(c(a(t(e)?)?)?)?)?)?)?)?|cla(s(s(i(f(y)?)?)?)?)?|occ(u(r(r(e(n(c(e(s)?)?)?)?)?)?)?)?|box|pars(e)?|wait|recv|tryrecv|hsv|utf(₈)?|graphemes|type|datetime|fft|json|csv|xlsx|binary|repr|pretty|voxels|&s|&pf|&p|&epf|&ep|&exit|&raw|&var|&runi|&runc|&runs|&cd|&sl|&invk|&cl|&fo|&fc|&fmd|&fde|&ftr|&fe|&fld|&fif|&fras|&frab|&ims|&ap|&tcpl|&tcpa|&tcpc|&tlsc|&tcpsnb|&tcpaddr|&fetch|&udpb|&udpr|&camcap|&memfree|graphemes|&memfree|&tcpaddr|datetime|&camcap|&tcpsnb|tryrecv|&fetch|voxels|pretty|binary|utf₈|&udpr|&udpb|&tlsc|&tcpc|&tcpa|&tcpl|&frab|&fras|&invk|&runs|&runc|&runi|&exit|&ims|&fif|&fld|&ftr|&fde|&fmd|&var|&raw|&epf|repr|xlsx|json|type|recv|wait|&ap|&fe|&fc|&fo|&cl|&sl|&cd|&ep|&pf|csv|fft|hsv|&p|&s)(?![a-zA-Z])|⋊[a-zA-Z]*"
		},
		"dyadic": {
			"name": "entity.name.function.uiua",
			"match": "[==≠<≤>≥+\\-×\\*÷%◿∨ⁿ↧↥∠ℂ≍⊟⊂⊏⊡↯↙↘↻⤸▽⌕⦷∊⨂⊥⍤]|(?<![a-zA-Z$])(eq(u(a(l(s)?)?)?)?|not (e(q(u(a(l(s)?)?)?)?)?)?|less( (t(h(a(n)?)?)?)?)?|le(s(s( (o(r( (e(q(u(a(l)?)?)?)?)?)?)?)?)?)?)?|greater( (t(h(a(n)?)?)?)?)?|greater o(r( (e(q(u(a(l)?)?)?)?)?)?)?|add|sub(t(r(a(c(t)?)?)?)?)?|mul(t(i(p(l(y)?)?)?)?)?|div(i(d(e)?)?)?|mod(u(l(o)?)?)?|or|pow(e(r)?)?|min(i(m(u(m)?)?)?)?|max(i(m(u(m)?)?)?)?|ata(n(g(e(n(t)?)?)?)?)?|com(p(l(e(x)?)?)?)?|gen|mat(c(h)?)?|cou(p(l(e)?)?)?|joi(n)?|sel(e(c(t)?)?)?|pic(k)?|res(h(a(p(e)?)?)?)?|tak(e)?|dro(p)?|rot(a(t(e)?)?)?|ori(e(n(t)?)?)?|kee(p)?|fin(d)?|mas(k)?|mem(b(e(r(o(f)?)?)?)?)?|ind(e(x(i(n)?)?)?)?|bas(e)?|ass(e(r(t)?)?)?|send|regex|map|has|get|remove|compress|bytes|img|gif|apng|layout|&rs|&rb|&ru|&w|&seek|&fwa|&gifs|&apngs|&tcpsrt|&tcpswt|&udpsml|&ffi|&memcpy|compress|&memcpy|&udpsml|&tcpswt|&tcpsrt|&apngs|layout|remove|&gifs|&seek|bytes|regex|&ffi|&fwa|apng|send|&ru|&rb|&rs|gif|img|get|has|map|gen|&w)(?![a-zA-Z])"
		},
		"mod1": {
			"name": "entity.name.type.uiua",
			"match": "[˙˜⊙⋅⟜⊸⤙⤚◠◡∩/∧\\\\≡⍚⊞⧅⧈⍥⊕⊜⧋◇∪⌅°⌝⍩⩜∂∫]|(?<![a-zA-Z$])(self|bac(k(w(a(r(d)?)?)?)?)?|dip|ga(p)?|on|by|wit(h)?|off|abo(v(e)?)?|bel(o(w)?)?|bot(h)?|dump|red(u(c(e)?)?)?|fol(d)?|sca(n)?|row(s)?|inv(e(n(t(o(r(y)?)?)?)?)?)?|tab(l(e)?)?|tup(l(e(s)?)?)?|st(e(n(c(i(l)?)?)?)?)?|rep(e(a(t)?)?)?|gro(u(p)?)?|par(t(i(t(i(o(n)?)?)?)?)?)?|eve(r(t)?)?|con(t(e(n(t)?)?)?)?|rea(c(h)?)?|obv(e(r(s(e)?)?)?)?|un|ant(i)?|cas(e)?|memo|comptime|spawn|pool|quote|geo(m(e(t(r(i(c)?)?)?)?)?)?|der(i(v(a(t(i(v(e)?)?)?)?)?)?)?|int(e(g(r(a(l)?)?)?)?)?|&rl|&ast|comptime|quote|spawn|&ast|pool|memo|dump|&rl)(?![a-zA-Z])"
		},
		"mod2": {
			"name": "keyword.control.uiua",
			"match": "[⊃⊓⍜⍢⬚⨬⍣]|(?<![a-zA-Z$])(for(k)?|bra(c(k(e(t)?)?)?)?|und(e(r)?)?|do|fil(l)?|sw(i(t(c(h)?)?)?)?|try|path|recur|recur|path)(?![a-zA-Z])"
		}
	},
	"scopeName": "source.uiua"
}